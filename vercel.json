{
    "version": 2,
    "routes": [
        {
            "src":"/(.*)",
            "dest":"api/index.py",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "functions": {
        "api/**/*": {
            "maxDuration": 20
        }
    }
}